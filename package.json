{
  "name": "webapp-kitchen-sink",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "lint": "biome check",
    "lint:write": "biome check --write",
    "format": "biome format --write .",
    "clean:modules": "bash -lc 'find . -type d -name node_modules -prune -print -exec rm -rf {} \\; && rm -f bun.lock'"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.0",
    "turbo": "^2.8.9",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "bun@1.3.9",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "elysia": "^1.4.25",
      "@types/bun": "^1.3.9"
    }
  }
}
